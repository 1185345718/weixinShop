/*搜索框样式设计*/
.top-list{
    width: 100%;
    z-index: 900;
}
.ssrq{
    display: none;
    margin:10px auto;
    width: 100%;
    height:30px;
    overflow: hidden;
    padding-top: 0px;
    background-size: 20px;
}
.search {
    margin-left: 20px;
    width: 73%;
    border: 1px solid #d1d1d1;
    border-radius:0.5em;
}
.box{
    margin:0px auto;
    width: 100%;
    height:40px;
    overflow: hidden;
    padding-top: 10px;
    background-size: 20px;
}
.ssrq span{
    font-size: 14px;
    color: white;
    margin-top: -24px;
    float: right;
    display: block;
    margin-right: 25px;
    font-family: "Microsoft YaHei";
}
.box .sous{

    background:transparent;
    margin-left: 21px;
    width: 89%;
    border: 1px solid #c3c3c3;
    border-radius:0.5em;
    font-size: 13px;
    line-height: 23px;
    color: #d9d9d9;
    height: 25px;
    text-align: center;
}
.search img{
    margin-left: 5px;
    width: 20px;
    vertical-align: middle;
    border: 0;
}
.box img{
    vertical-align: middle;
    border: 0;
    width: 16px;
}
.search input{
    outline: none;
    appearance:none;
    background:transparent;
    margin-left: 1px;
    width: 86%;
    border: 0px;
    font-size: 13px;
    color: #d9d9d9;
    height: 25px;
}
input[type=search]::-webkit-search-cancel-button{
    /*-webkit-appearance: none;//此处只是去掉默认的小×*/
    color: #c3c3c3;
    -webkit-appearance: none;
}
.class-main2 .left-list{
    top: 50px;
}
.class-main2 .right-list{
    position: fixed;
    top: 50px;
}
.mb{
    display: none;
}
#cnt{
    left: -90px;
    width: 123%;

}

.yhq{
    width: 100%;
    height: 100%;
    background-color: rgba(255, 255, 255, 0.3);
    position: fixed;
    z-index: 15;
}
.yhqimg{
    position: relative;
    margin: 0 auto;
    /*margin-top: 10em;*/
    width: 280px;
    height: 223px;
    background: url("../image/yhq.png") white center no-repeat;
    background-size: 22em;
}
.yhqimg span{
    display: block;
    position: absolute;
    top: 75px;
    left: 115px;
    color: #ec4d35;
    font-weight: 900;
    font-size: 24px;
    line-height: 20px;
    font-family: "微软雅黑";
}
.yhqimg span i{
    font-family: "微软雅黑";
    display: block;
    float: left;
    font-size: 28px;
}
.yhqhz{
    border-radius: 10px;
    background-color: white;
    width: 300px;
    margin: 0 auto;
    position: absolute;
    left: 50%;
    top: 49%;
    margin-top:-50%;margin-left:-150px;
}
.yhqtext{
    margin: 0 auto;
    text-align: center;
    width: 280px;
    background-color: white;
    color: #AC4C1E;
    font-weight:bold;
    padding-bottom: 12px;
}
.btn{
    width: 280px;
    background-color: white;
    margin: 0 auto;
    text-align: center;
    padding-bottom: 30px;
}
.btn button{
    text-align: center;
    margin: 0 auto;
    width: 200px;
    color: #FFEA97;
    font-size: 20px;
    background-color: #F15D35;
    height: 50px;
    border:6px solid #FFD864;
    border-radius: 20em;
}



/*内容样式*/

#wrapper{
    width: 25%;
    height: 677px;
    float: left;
    margin-right: 3px;
    position:relative;
    z-index:1;
    top:0;
    bottom:48px;
    left:0;
    background:#FAFAFA;
    overflow:auto;
}


#transition {
    position:relative;
    z-index:1;
    display:block;
    float:left;
    width:73%;
    height: 677px;
    overflow:auto;
background-color: #FFFFFF;
    float: left;
}
#wrapper2 {
    width: 100%;
    z-index: 8;
    height:600px;
    box-sizing: border-box;
    position:relative;
    overflow: hidden;
    left: 0;
    top: 0;
}


.scroller {
    position:absolute;
    z-index:1;
    width:100%;
    padding:0;
}

.scroller ul {
    list-style:none;
    padding:0;
    margin:0;
    width:100%;
    text-align:left;
}
.box1 li{
position: relative;
    padding:0 10px;
    height:42px;
    line-height:40px;
    border-bottom:2px solid #F3F0F0;
    font-size:14px;
    color: black;
    text-align: center;
}
.box2 li{
    position: relative;

}
.box2 li a {
    display: block;
    width: 100%;
    padding:10px 10px;
    height: 4.2rem;
    background: rgba(255, 255, 255, 0.2);
    border-bottom:2px solid #F3F0F0;
font-style: normal;
    font-size:14px;
color: #000000;
    text-align: center;
    overflow: hidden;
}
.bul{
    position: absolute;
    /*height: 100%;*/
    overflow: visible;
    white-space: nowrap;
    margin-top:4.6rem;
}
.box3{

}
.box3 li{
    display: inline-block;
    width:15rem;

    height: 50px;
    white-space: normal;
}

.box3 li img{
    width: 100%;
}


.pic {
    float: left;
    margin-right: 10px;
}
.pic img{
    width: 4rem;

}
.text{
    float: left;
}
.text i{
    font-style:normal;
    width: 1rem;
    height: 1rem;
    line-height: 1rem;
    color: #000;
    text-align: center;
    background: #fd9c46;
    display: inline-block;
    border-radius: 19.98rem;
    margin-right: 0.32rem;
}
.text .price{
    color: #fd9c46;
    text-align: left;
    margin-top: 31px;
}
.text .price span{
   display: block;
}
.price span:nth-child(1){
    color:#999999 ;
}
.add-icon {
    position: absolute;
    right: 0;
    top: 0;
    top: 3rem;
    text-align: right;
    margin-right: 0.2rem;
}
.add-icon i{
    width: 1.2rem;
    height: 1.2rem;
    display: inline-block;
    margin-top: 0.4rem;

}
.add-icon .jia{
    background: url(../image/shop_jia.png) no-repeat;
    background-size: 100%;
}
.add-icon .jian{
    display: none;
    background: url(../image/shop_jian.png) no-repeat;
    background-size: 100%;
}
.num{
    width: 1.18rem;
    border: 0;
    background: transparent;
    text-align: center;
    height: 1.18rem;
    vertical-align: top;
    margin-top: 0.4rem;
}
.shop-server{

    position: absolute;
    width: 2.2rem;display: none;
    height: 2.2rem;
    background: #242322;
    z-index: 9;
    left:7rem;
    border-radius: 50%;
    font-size: 0.8rem;
    text-align: center;
    line-height: 1.2rem;
    padding: 0.26rem;
    box-sizing: border-box;
    color: #fff;
    border: 1px solid #fff;
    text-decoration:none;
}
.settlement{
    width: 4.8rem;
    height:2.2rem;
    display: block;
    float: right;
    z-index: 9;
    font-weight: 900;
    background-color: #fff032;
   line-height: 2.2rem;
    font-size:1rem;
    text-align: center;
    box-sizing: border-box;
    color: #232323;
    text-decoration:none;
}

.shop-gwc{
    position: absolute;
    bottom: 0.8rem;
    width: 2.2rem;
    height: 2.2rem;
    background: url(../image/shopCar.png) no-repeat #ff8205 center center;
    border-radius: 50%;
    background-size: 60% auto;
    z-index: 9;
    /*left: 1rem;*/
    left: -5rem;
    border: 1px solid #fff;
    box-sizing: border-box;

}
.shop-gwc i{
    position: absolute;
    text-indent: 0;
    top: -0.1rem;
    right: -0.3rem;
    line-height: 0.9rem;
    font-size: 0.5rem;
    border: 1px solid #fff;
    text-align: center;
    background: #ff8205;
    border-radius: 19.98rem;
    width: 0.9rem;
    height: 0.9rem;
    color: #fff;
}
.total-price{
    position: absolute;
    width: 5.5rem;

    font-size: 0.6rem;
    color: #fff;
    right: 1rem;

    padding-left: 0.2rem;
    height: 3rem;
   margin-right: 3rem;
    text-indent: 0.5rem;

    box-sizing: border-box;
    z-index: 8;
    font-size: 0.8rem;
    text-align: left;
}
.total-price i{
    margin-left: 8px;
}

.total-price i:nth-child(3){
  color: #ff8205;
    font-size: 16px;
}
.foder{
    width: 100%;
    height: 2.2rem;
    position: fixed;
    z-index: 10;
    bottom: 0rem;
    background-color: #242322;;
}

#floatOrder {
    width: 20px;
    height: 20px;
    padding: 2px;
    background: rgba(255, 255, 255, 0.5);
    border: solid 2px #e54144;
    overflow: hidden;
    position: absolute;
    z-index: 890;
    border-radius: 10px;
}